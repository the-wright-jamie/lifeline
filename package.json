{
  "name": "lifeline",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "homepage": "https://the-wright-jamie.dev/lifeline",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "chart.js": "^*",
    "endent": "^2.1.0",
    "fuse.js": "^*",
    "vue": "^*",
    "vue-mermaid-string": "^7.0.0",
    "vue-router": "^*"
  },
  "devDependencies": {
    "gh-pages": "^*",
    "@rushstack/eslint-patch": "^*",
    "@tsconfig/node20": "^*",
    "@types/node": "^*",
    "@vitejs/plugin-vue": "^*",
    "@vue/eslint-config-prettier": "^*",
    "@vue/eslint-config-typescript": "^*",
    "@vue/tsconfig": "^*",
    "autoprefixer": "^*",
    "eslint": "^*",
    "eslint-plugin-vue": "^*",
    "npm-run-all2": "^*",
    "postcss": "^*",
    "prettier": "^*",
    "tailwindcss": "^*",
    "typescript": "^*",
    "vite": "^*",
    "vue-tsc": "^*"
  }
}
